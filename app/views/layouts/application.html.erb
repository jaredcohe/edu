<!DOCTYPE html>
<html>
<head>
  <title>Edu</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= notice %>
  <%= alert %>
  
<%= link_to "Edu", :root %>
  <% if current_user %>
    <%= current_user.email %>
    <%= link_to "Log Out", logout_path, method: "delete" %>
  <% else %>
    <%= link_to "Sign Up", signup_path %>
    <%= link_to "Log In", login_path %>
  <% end %>

<%= yield %>

</body>
</html>
